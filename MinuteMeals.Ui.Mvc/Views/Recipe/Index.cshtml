@model IList<Recipe>
@{
    ViewData["Title"] = "Recipes";
}

<h1>Recipes</h1>

<table class="table table-dark table-striped">
    <thead>
    <tr>
        <th>Name</th>
        <th>Cooking Time</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var recipe in Model)
    {
        <partial name="RecipeRowPartial" model="recipe"/>
    }
    </tbody>
</table>